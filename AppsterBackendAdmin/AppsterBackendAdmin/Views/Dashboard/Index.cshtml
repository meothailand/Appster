@model AppsterBackendAdmin.Models.View.DashboardViewModel

@section bodyContent
{
<div class="main_heading">
    <h2 class="left">Recently Added Users</h2>
    <div class="right">
        <a href="#" title="add user">Add User</a>
    </div>
</div>
<div class="row">
    @Html.Partial("_NewAddedUserPartial", Model)
</div>

@if (Model.NewAddedAdmins.Count > 0)
{
    <div class="main_heading">
        <h2 class="left">Recently Added Sub-Admin</h2>
        <div class="right">
            <a href="#" title="add sub-admin">Add Sub-Admin</a>
        </div>
    </div>
    <div class="row">
        @Html.Partial("_NewAddedAdminUserPartial", Model)
    </div>
}

<div class="main_heading">
    <h2 class="left">Newsfeed</h2>
</div>
<div class="row">
    @Html.Partial("_NewsFeedPartial", Model)
</div>
<div class="main_heading">
    <h2 class="left">New Added Gifts</h2>
    <div class="right">
        <a href="#" title="add gift">Add Gift</a>
    </div>
</div>
<div class="row">
    @Html.Partial("_NewAddedGiftPartial", Model)
</div>
}

@section customScripts
{
    <script type="text/javascript" src="~/Scripts/page.dashboard.js"></script>
}